{
  "name": "manifest",
  "version": "6.4.0",
  "description": "Universal App Store Service - Containerized, modular service for managing applications across all ecosystems with version management, CI/CD integration, and installation automation",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "lint": "eslint src/",
    "build": "tsc",
    "clean": "rm -rf dist/",
    "docker:build": "docker build -t manifest .",
    "docker:run": "docker run -p 3000:3000 manifest",
    "cli": "node src/cli/manifest-cli.js",
    "manifest": "node src/cli/manifest-cli.js",
    "push": "./push.sh",
    "push:patch": "./push.sh patch",
    "push:minor": "./push.sh minor",
    "push:major": "./push.sh major",
    "install:cli": "./install-local-cli.sh"
  },
  "keywords": [
    "manifest",
    "versioning",
    "documentation",
    "git",
    "github",
    "universal",
    "app-store",
    "cicd",
    "containerization",
    "deployment",
    "automation",
    "multi-platform",
    "multi-ecosystem"
  ],
  "author": "Kanizsa Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "simple-git": "^3.20.0",
    "semver": "^7.5.4",
    "yaml": "^2.3.4",
    "js-yaml": "^4.1.0",
    "marked": "^9.1.6",
    "gray-matter": "^4.0.3",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "boxen": "^7.1.1",
    "update-notifier": "^6.0.2",
    "node-cron": "^3.0.3",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "@types/node": "^20.10.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kanizsa/manifest.git"
  },
  "bugs": {
    "url": "https://github.com/kanizsa/manifest/issues"
  },
  "homepage": "https://github.com/kanizsa/manifest#readme"
}
